@model TinyMceEditorModel

<script src="~/lib/tinymce/tinymce.min.js"></script>
<script>
    tinymce.init({
        selector: '#@Model.EditorId',
        height: '@Model.Height',
        plugins: '@Model.Plugins',
        toolbar: '@Model.Toolbar',
        menubar: '@Model.Menubar',
        resize: '@Model.Resize',
        paste_as_text: '@Model.PasteAsText',
        license_key: 'gpl'
    });
</script>